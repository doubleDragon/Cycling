<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <attr name="indicatorBarTabStyle" format="reference" />
    <attr name="indicatorTabBarStyle" format="reference" />
    <attr name="indicatorBarTabTextStyle" format="reference"/>
    
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    
    <!-- Attributes for RoundedCornerImageView -->
    <declare-styleable name="RoundedCornerImageView">
        <!-- Corner radius -->
        <attr name="cornerRadius" format="dimension" />
    </declare-styleable>
    
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color_color" format="color" />
    </declare-styleable>
</resources>
